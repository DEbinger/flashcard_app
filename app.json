{
  "expo": {
    "name": "FlashCards",
    "description": "Flashcard application to allow for added categories and questions. To be used in a quiz in app.",
    "slug": "FlashCards",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidebinger.flashcards"
    },
    "android": {
      "package": "com.davidebinger.flashcards"
    }
  }
}